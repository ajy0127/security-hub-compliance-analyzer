{
    "custom_controls": {
        "ORG.SEC.1": "Organization-specific security control for data protection",
        "ORG.SEC.2": "Organization-specific security control for access management",
        "ORG.SEC.3": "Organization-specific security control for encryption requirements",
        "ORG.SEC.4": "Organization-specific security control for network security",
        "ORG.SEC.5": "Organization-specific security control for logging and monitoring",
        "ORG.COMP.1": "Organization-specific compliance control for regulatory requirements",
        "ORG.COMP.2": "Organization-specific compliance control for data retention",
        "ORG.COMP.3": "Organization-specific compliance control for privacy requirements"
    },
    "regex_mappings": {
        "encr(y|i)ption": {
            "primary_controls": ["ORG.SEC.3", "CC6.1.7"],
            "secondary_controls": ["CC6.1.8", "CC6.1.10"]
        },
        "password|credentials|secret": {
            "primary_controls": ["ORG.SEC.2", "CC6.1.2", "CC6.1.3"],
            "secondary_controls": ["CC6.2.1"]
        },
        "log(ging)?|monitor(ing)?|alert": {
            "primary_controls": ["ORG.SEC.5", "CC6.6.2", "CC6.6.4"],
            "secondary_controls": ["CC7.1.2", "CC7.1.3"]
        },
        "network|firewall|security.?group|nacl": {
            "primary_controls": ["ORG.SEC.4", "CC6.1.4", "CC6.6.3"],
            "secondary_controls": ["CC6.6.2"]
        },
        "data.protection|gdpr|ccpa|privacy": {
            "primary_controls": ["ORG.COMP.3", "C1.1"],
            "secondary_controls": ["CC6.1.10", "CC6.7"]
        },
        "retention|backup|archive": {
            "primary_controls": ["ORG.COMP.2"],
            "secondary_controls": ["CC6.1.7"]
        }
    },
    "resource_mappings": {
        "AwsS3Bucket": {
            "primary_controls": ["ORG.SEC.1", "CC6.1.10", "CC6.1.7"],
            "secondary_controls": ["ORG.COMP.2", "CC6.1.4"]
        },
        "AwsIamRole": {
            "primary_controls": ["ORG.SEC.2", "CC6.1.2", "CC6.1.3"],
            "secondary_controls": ["CC6.1.9", "CC6.2.1"]
        },
        "AwsKmsKey": {
            "primary_controls": ["ORG.SEC.3", "CC6.1.8", "CC6.1.7"],
            "secondary_controls": ["CC6.1.10"]
        },
        "AwsEc2SecurityGroup": {
            "primary_controls": ["ORG.SEC.4", "CC6.1.4", "CC6.6.3"],
            "secondary_controls": ["CC6.6.2"]
        },
        "AwsCloudTrail": {
            "primary_controls": ["ORG.SEC.5", "CC6.6.2", "CC6.6.4"],
            "secondary_controls": ["CC7.1", "CC7.2"]
        },
        "AwsConfig": {
            "primary_controls": ["ORG.SEC.5", "CC6.6.2", "CC6.6.4"],
            "secondary_controls": []
        },
        "AwsRdsDbInstance": {
            "primary_controls": ["ORG.SEC.1", "ORG.SEC.3", "CC6.1.7", "CC6.1.8"],
            "secondary_controls": ["CC6.1.4", "CC6.6.2"]
        }
    }
}