[flake8]
max-line-length = 100
exclude = venv/*,*.pyc,__pycache__/*
per-file-ignores =
    # Ignore unused imports in __init__.py files
    __init__.py:F401
    # Ignore f-string placeholders in test files
    tests/*:F541
    # Ignore line length in test files
    tests/*:E501
    # Ignore unused imports in test files
    tests/*:F401 